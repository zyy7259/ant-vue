{
  "name": "ant-vue",
  "version": "0.0.1",
  "description": "antd with vue",
  "main": "dist/index.js",
  "scripts": {
    "css:lib": "postcss \"src/style/**/*.css\" --dir lib --base src",
    "css:es": "postcss \"src/style/**/*.css\" --dir es --base src",
    "css": "run-p -n css:*",
    "compile": "babel src --out-dir lib && NO_MODULES=true babel src --out-dir es",
    "clean": "rimraf dist lib",
    "babel": "babel src --out-dir lib",
    "dev": "npm run clean && (cross-env NODE_ENV=development webpack-dev-server --color --info=false & npm run babel -- -w & npm run css -- -w)",
    "see": "npm run clean && cross-env NODE_ENV=development webpack && npm run babel && npm run css",
    "build": "npm run clean && cross-env NODE_ENV=production webpack && npm run babel && npm run css",
    "prepublish": "npm run build",
    "lintjs": "eslint \"src/**/*.js\"",
    "formatjs": "prettier-eslint \"src/**/*.js\" --write",
    "lintcss": "stylelint \"src/**/*.css\"",
    "formatcss": "stylelint \"src/**/*.css\" --fix",
    "-precommit": "lint-staged",
    "-commitmsg": "commitlint -e",
    "template-script-css": "echo 233"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zyy7259/ant-vue.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zyy7259/ant-vue/issues"
  },
  "homepage": "https://github.com/zyy7259/ant-vue#readme",
  "devDependencies": {
    "@commitlint/cli": "^5.2.5",
    "@commitlint/config-angular": "^5.1.1",
    "autoprefixer": "^7.2.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.3.0",
    "copy-webpack-plugin": "^4.3.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "cssnano": "4.0.0-rc.2",
    "eslint": "^4.13.1",
    "eslint-config-zoro-node": "^0.1.203",
    "eslint-config-zoro-vue": "^0.1.204",
    "execa": "^0.9.0",
    "extract-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-loader": "^0.5.1",
    "husky": "^0.14.3",
    "internal-ip": "^3.0.1",
    "lint-staged": "^6.0.0",
    "node-notifier": "^5.2.1",
    "postcss": "^6.0.14",
    "postcss-cli": "^4.1.1",
    "postcss-inline-svg": "^3.0.0",
    "postcss-loader": "^2.0.9",
    "postcss-pxtorem": "^4.0.1",
    "precss": "^2.0.0",
    "prettier-eslint-cli": "^4.6.1",
    "prettier-stylelint": "^0.4.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.3",
    "stats-webpack-plugin": "^0.6.1",
    "style-loader": "^0.19.1",
    "stylelint": "^8.3.1",
    "stylelint-config-zoro": "^0.1.228",
    "url-loader": "^0.6.2",
    "watch": "^1.0.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-middleware": "^2.0.1",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1",
    "yaml-loader": "^0.5.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "date-fns": "^1.29.0",
    "es6-promise": "^4.1.1",
    "eventemitter3": "^3.0.0",
    "lodash": "^4.17.4",
    "object.assign": "^4.1.0",
    "qs": "^6.5.1"
  }
}
